#!/home/fja0568/src/brownbear/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Send2Trash','console_scripts','send2trash'
__requires__ = 'Send2Trash'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Send2Trash', 'console_scripts', 'send2trash')()
    )
