#!/home/fja0568/src/brownbear/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'jupyter-client','console_scripts','jupyter-kernelspec'
__requires__ = 'jupyter-client'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('jupyter-client', 'console_scripts', 'jupyter-kernelspec')()
    )
